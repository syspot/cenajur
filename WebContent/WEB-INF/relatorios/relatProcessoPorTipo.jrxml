	<parameter name="DATA_INICIAL" class="java.util.Date"/>
	<parameter name="DATA_FINAL" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT TP.DESCRICAO,
       (SELECT COUNT(P.*) FROM PROCESSOS P WHERE P.DATA_CADASTRO BETWEEN COALESCE($P{DATA_INICIAL}, P.DATA_CADASTRO) AND COALESCE($P{DATA_FINAL}, P.DATA_CADASTRO) AND P.TIPO_PROCESSO_ID = TP.ID AND P.SITUACAO_PROCESSO_ID = 1) AS QTD_ATIVO,
       (SELECT COUNT(P.*) FROM PROCESSOS P WHERE P.DATA_CADASTRO BETWEEN COALESCE($P{DATA_INICIAL}, P.DATA_CADASTRO) AND COALESCE($P{DATA_FINAL}, P.DATA_CADASTRO) AND P.TIPO_PROCESSO_ID = TP.ID AND P.SITUACAO_PROCESSO_ID = 2) AS QTD_SUSPENSO,
       (SELECT COUNT(P.*) FROM PROCESSOS P WHERE P.DATA_CADASTRO BETWEEN COALESCE($P{DATA_INICIAL}, P.DATA_CADASTRO) AND COALESCE($P{DATA_FINAL}, P.DATA_CADASTRO) AND P.TIPO_PROCESSO_ID = TP.ID AND P.SITUACAO_PROCESSO_ID = 3) AS QTD_ARQUIVADO
 FROM TIPOS_PROCESSOS TP]]>
	</queryString>
