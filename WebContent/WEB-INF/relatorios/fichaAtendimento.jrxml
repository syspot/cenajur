	<parameter name="P_AGENDA_COLABORADOR_ID" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
       C.ID, C.NOME, C.DATA_ADESAO, COALESCE(C.TELEFONE || ', ', ' ') || COALESCE(C.CELULAR || '', ' ') AS TELEFONES, C.MATRICULA,
       COALESCE(C.LOGRADOURO || ', ', '') || COALESCE('NUMERO ' || C.NUMERO, '') || COALESCE(C.COMPLEMENTO || ', ', '')
       || COALESCE(C.BAIRRO || ', ', '') || COALESCE(CID.DESCRICAO || '/' || E.DESCRICAO || ', ', '') || COALESCE('CEP ' || C.CEP, '') AS ENDERECO,
       COALESCE(AC.TEXTO_RESPOSTA, '') AS TEXTO_RESPOSTA, COL.NOME AS NOME_ADVOGADO
       FROM AGENDAS A
       INNER JOIN AGENDAS_COLABORADORES AC ON AC.AGENDA_ID = A.ID
       INNER JOIN COLABORADORES COL ON COL.ID = AC.COLABORADOR_ID
       INNER JOIN CLIENTES C ON C.ID = A.CLIENTE_ID
       INNER JOIN CIDADES CID ON C.CIDADE_ID = CID.ID
       INNER JOIN ESTADOS E ON E.ID = CID.ESTADO_ID
       WHERE AC.ID = $P{P_AGENDA_COLABORADOR_ID}]]>
	</queryString>
