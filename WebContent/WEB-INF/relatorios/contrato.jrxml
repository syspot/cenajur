					</textElement>
					<textFieldExpression><![CDATA["<style isBold=\"true\" pdfFontName=\"Helvetica-Bold\">CLÁUSULA SEXTA:</style>"]]></textFieldExpression>
				</textField>
				<textField>
					<reportElement uuid="068aa1e9-7a6e-4e30-a24d-58ceefb9a0c7" x="46" y="563" width="471" height="20"/>
